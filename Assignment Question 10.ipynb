    "#10 Collaatz sequence\n",
    "\n",
    "n = int(input(\"input the number\"))\n",
    "s = [n]\n",
    "while n != 1:\n",
    "    if n % 2 == 0:\n",
    "        n = n // 2\n",
    "    else:\n",
    "        n = 3 * n + 1\n",
    "    s.append(n)  \n",
    "\n",
    "print(s)"
